{
  "name": "domutils",
  "version": "1.5.1",
  "description": "TypeScript compatible utilities for working with htmlparser2's dom",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "mocha test/tests/**.js && jshint index.js test/**/*.js lib/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/donnut/domutils.git"
  },
  "keywords": [
    "dom",
    "htmlparser2"
  ],
  "dependencies": {
    "dom-serializer": "git://github.com/donnut/dom-serializer",
    "domelementtype": "git://github.com/donnut/domelementtype"
  },
  "devDependencies": {
    "htmlparser2": "git://github.com/donnut/htmlparser2",
    "domhandler": "git://github.com/donnut/domhandler",
    "jshint": "~2.3.0",
    "mocha": "~1.15.1"
  },
  "author": "Felix Boehm <me@feedic.com>",
  "jshintConfig": {
    "proto": true,
    "unused": true,
    "eqnull": true,
    "undef": true,
    "quotmark": "double",
    "eqeqeq": true,
    "trailing": true,
    "node": true,
    "globals": {
      "describe": true,
      "it": true,
      "beforeEach": true
    }
  }
}
